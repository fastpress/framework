{
    "name": "fastpress/framework",
    "description": "A comprehensive framework encompassing all essential components for building web applications with Fastpress.",
    "type": "library",
    "version": "0.1.4",
    "require": {
        "php": "^7.4 || ^8.0",
        "fastpress/request": "0.1.*",
        "fastpress/response": "0.1.*",
        "fastpress/router": "0.1.*",
        "fastpress/session": "0.1.*",
        "fastpress/view": "0.1.*",
        "fastpress/database": "0.1.*"
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "phpcbf --standard=PSR12 src/",
        "lint": "phpcs --standard=PSR12 src/",
        "analyze": "phpstan analyse src/"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fastpress\\": "src/",
            "App\\Controller\\": "src/app/controller/",
            "App\\Service\\": "src/app/service/",
            "App\\Repository\\": "src/app/repository/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "samayo",
            "email": "samayo@pm.me"
        }
    ],
    "keywords": [
        "framework",
        "fastpress",
        "web application"
    ],
    "homepage": "https://github.com/fastpress/framework",
    "support": {
        "issues": "https://github.com/fastpress/framework/issues",
        "source": "https://github.com/fastpress/framework"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4" 
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.x-dev" 
        }
    }
}