{
    "name": "fastpress/framework",
    "description": "A comprehensive framework encompassing all essential components for building web applications with Fastpress.",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "fastpress/request": "dev-main",
        "fastpress/response": "dev-main",
        "fastpress/router": "dev-main",
        "fastpress/session": "dev-main",
        "fastpress/view": "dev-main"
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "phpcbf --standard=PSR12 src/",
        "lint": "phpcs --standard=PSR12 src/",
        "analyze": "phpstan analyse src/"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fastpress\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "keywords": ["framework", "fastpress", "web application"],
    "homepage": "https://github.com/fastpress/framework",
    "support": {
        "issues": "https://github.com/fastpress/framework/issues",
        "source": "https://github.com/fastpress/framework"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.0"
        }
    }
}
